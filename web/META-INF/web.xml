<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>AccountServlet</servlet-name>
        <servlet-class>controllers.AccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateMenteeProfile</servlet-name>
        <servlet-class>controllers.UpdateMenteeProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangPassServlet</servlet-name>
        <servlet-class>controllers.ChangPassServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controllers.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountServlet</servlet-name>
        <url-pattern>/AccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateMenteeProfile</servlet-name>
        <url-pattern>/UpdateMenteeProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangPassServlet</servlet-name>
        <url-pattern>/ChangPassServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
</web-app>
